page {
  background-color: #dedede;
  padding: 15rpx;
}
.search_row {
  display: flex;
  height: 70rpx;
}
.search_row input {
  flex: 1;
  height: 100%;
  padding-left: 20rpx;
  background-color: #f8f8f8;
  border-radius: 8rpx;
}
.search_row button {
  width: 110rpx;
  height: 100%;
  padding: 0;
  margin-left: 10rpx;
  font-weight: 400;
  font-size: 26rpx;
  text-align: center;
  line-height: 70rpx;
  color: #666;
}
.search_content {
  margin-top: 15rpx;
}
.search_content navigator {
  padding: 20rpx 15rpx;
  margin-bottom: 6rpx;
  background-color: #f8f8f8;
  font-size: 26rpx;
  border-radius: 4rpx;
  overflow: hidden;
  white-space: nowrap;
  text-overflow: ellipsis;
}
